
<form action="<%= users_url %>" method="POST">
<%= render 'layouts/auth', form_authenticity_token: session[:session_token]%>
  <label for="user_username">Username</label>
  <input
      type="text"
      name="user[username]"
      id="user_username">
  <br>

  <label for="user_password">Password</label>
    <input
      type="password"
      name="user[password]"
      id="user_password">
  <br>

  <button>create</button>
</form>
